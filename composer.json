{
  "name": "alariva/tidiochat",
  "description": "A minimal package for integrating TidioChat.com to your webapp",
  "keywords": [],
  "homepage": "https://alariva.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Ariel Vallese",
      "email": "alariva@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "~5.2",
    "illuminate/support": "~5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "Alariva\\Tidiochat\\": "src\\"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alariva\\Tidiochat\\TidioChatServiceProvider"
      ],
      "aliases": {
          "TidioChat": "Alariva\\Tidiochat\\Facades\\TidioChat"
      }
    }
  },
  "minimum-stability": "dev"
}
